<h2>Create a Listings</h2>


<%= form_with scope: :listing, url: listings_path, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :place_type %>
    <%= form.text_field :place_type %>
  </div>

  <div>
    <%= form.label :property_type %>
    <%= form.text_field :property_type %>
  </div>

  <div>
    <%= form.label :room_number %>
    <%= form.text_field :room_number %>
  </div>

  <div>
    <%= form.label :bed_number %>
    <%= form.text_field :bed_number %>
  </div>

  <div>
    <%= form.label :guest_number %>
    <%= form.text_field :guest_number %>
  </div>

  <div>
    <%= form.label :country %>
    <%= form.text_field :country %>
  </div>

  <div>
    <%= form.label :state %>
    <%= form.text_field :state %>
  </div>

  <div>
    <%= form.label :city %>
    <%= form.text_field :city %>
  </div>

  <div>
    <%= form.label :zipcode %>
    <%= form.text_field :zipcode %>
  </div>

  <div>
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div>
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.submit "Create Listing" %>
  </div>
<% end %>
